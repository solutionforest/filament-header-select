{
    "name": "solution-forest/filament-header-select",
    "description": "A simple, powerful header select component for Filament admin panels",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Solution Forest",
        "email": "hello@solutionforest.com"
    }],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0|^4.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^8.0|^9.0",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "SolutionForest\\FilamentHeaderSelect\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SolutionForest\\FilamentHeaderSelect\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SolutionForest\\FilamentHeaderSelect\\HeaderSelectServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
